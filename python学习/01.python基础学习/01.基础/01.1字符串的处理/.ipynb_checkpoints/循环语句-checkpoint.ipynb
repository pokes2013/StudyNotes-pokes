   "metadata": {},
   "source": [
    "实例：打印1-5的数字？"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "cc834064",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1\n",
      "2\n",
      "3\n",
      "4\n",
      "5\n"
     ]
    }
   ],
   "source": [
    "a=1\n",
    "while a<=5:\n",
    "    print(a)\n",
    "    a=a+1"
